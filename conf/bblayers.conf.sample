  ${HOME}/yocto/opendash/poky-dunfell/meta-openembedded/meta-oe \
  ${HOME}/yocto/opendash/poky-dunfell/meta-openembedded/meta-multimedia \
  ${HOME}/yocto/opendash/poky-dunfell/meta-openembedded/meta-networking \
  ${HOME}/yocto/opendash/poky-dunfell/meta-openembedded/meta-perl \
  ${HOME}/yocto/opendash/poky-dunfell/meta-openembedded/meta-python \
  ${HOME}/yocto/opendash/poky-dunfell/meta-qt5 \
  ${HOME}/yocto/opendash/poky-dunfell/meta-raspberrypi \
  ${HOME}/yocto/opendash/poky-dunfell/meta-security \
  ${HOME}/yocto/opendash/poky-dunfell/meta-crankshaft/meta-crankshaft \
  ${HOME}/yocto/opendash/poky-dunfell/meta-opendash \
  ${HOME}/yocto/opendash/poky-dunfell/meta-mender/meta-mender-core \
